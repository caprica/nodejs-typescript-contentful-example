{
    "version": "0.1.0",
    "configurations": [
      {
        "name": "Teardown command",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "runtimeArgs": ["-r", "ts-node/register", "--preserve-symlinks", "--nolazy"],
        "args": ["${workspaceRoot}/src/main.ts", "teardown"],
        "outputCapture": "std"
      },
      {
        "name": "Setup command",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "runtimeArgs": ["-r", "ts-node/register", "--preserve-symlinks", "--nolazy"],
        "args": ["${workspaceRoot}/src/main.ts", "setup"],
        "outputCapture": "std"
      }
    ]
  }